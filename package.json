{
  "name": "node-react-webpack-boilerplate",
  "version": "1.0.0",
  "description": "react enviorment and project production",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint **/js/",
    "lint-fix": "./node_modules/.bin/eslint --fix **/js/*.js",
    "test": "./node_modules/mocha/bin/mocha --recursive --compilers js:babel-core/register **/test/",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --inline --line --hot --content-base dist --history-api-fallback",
    "start:prod": "echo \"Error: no production server\" && exit 1",
    "build": "webpack"
  },
  "keywords": [
    "react",
    "jslint",
    "redux",
    "mocha"
  ],
  "author": "wmx",
  "license": "ISC",
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.9",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.11.1",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "if-env": "^1.0.0",
    "mocha": "^3.2.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
